<?xml version="1.0"?>
<odoo>
    <record id="auto_word_wind_turbines_form" model="ir.ui.view">
        <field name="name">风机信息表</field>
        <field name="model">auto_word_wind.turbines</field>
        <field name="arch" type="xml">
            <form string="风机信息表">
                <sheet>
                    <group name="group_top">
                         <group name="group_left">
                            <field name="name_tur" />
                            <field name="capacity" />
                            <field name="rotor_diameter" />
                            <field name="rotor_swept_area" />
                            <field name="hub_height" />
                             <field name="tower_type" />
                             <field name="tower_weight" />
                         </group>
                         <group name="group_right">
                            <field name="power_regulation" />
                            <field name="generator_type" />
                            <field name="rated_wind_speed" />
                            <field name="cut_in_wind_speed" />
                            <field name="cut_out_wind_speed" />
                             <field name="three_second_maximum" />
                        </group>
                    </group>
                    <!--<group name="group_bottom">-->
                        <!--<field name="generator_Output" />-->
                    <!--</group>-->
                </sheet>
            </form>
        </field>
    </record>

    <record id="wind_turbines_case_form" model="ir.ui.view">
        <field name="name">风机方案表</field>
        <field name="model">wind_turbines.case</field>
        <field name="arch" type="xml">
            <form string="风机信息表">
                <sheet>
                    <group name="group_top">
                         <group name="group_left">
                            <field name="name_tur" />
                            <field name="capacity" />
                            <field name="rotor_diameter" />
                            <field name="rotor_swept_area" />
                            <field name="hub_height" />
                             <field name="tower_type" />
                             <field name="tower_weight" />
                             <field name="power_regulation" />
                            <field name="generator_type" />

                         </group>
                         <group name="group_right">
                            <field name="rated_wind_speed" />
                            <field name="cut_in_wind_speed" />
                            <field name="cut_out_wind_speed" />
                            <field name="three_second_maximum" />
                            <field name="turbine_numbers" />
                            <field name="investment_turbines_kw" />
                            <!--<field name="case_hub_height" />-->
                         </group>
                    </group>
                    <!--<group name="group_bottom">-->
                        <!--<field name="generator_Output" />-->
                    <!--</group>-->
                </sheet>
            </form>
        </field>
    </record>

    <record id="wind_turbines_case_list" model="ir.ui.view">
        <field name="name">风机方案表</field>
        <field name="model">wind_turbines.case</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name_tur"/>
                <field name="capacity" />
                <field name="rotor_diameter" />
                <field name="rotor_swept_area" />
                <field name="turbine_numbers" />

            </tree>
        </field>
    </record>


