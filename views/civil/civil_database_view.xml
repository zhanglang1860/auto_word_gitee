                            <field name="StoneMasonryFoot" />
                            <field name="StoneMasonryDrainageDitch" />
                            <field name="RoadArea" />
                            <field name="GreenArea" />
                            <field name="LightningRod" />
                        </group>
                         <group name="group_right">
                            <field name="ComprehensiveBuilding" />
                            <field name="EquipmentBuilding" />
                            <field name="AffiliatedBuilding" />
