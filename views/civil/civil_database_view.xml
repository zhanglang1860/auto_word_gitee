<?xml version="1.0"?>
<odoo>
    <record id="auto_word_civil_windbase_form" model="ir.ui.view">
        <field name="name">风机基础数据</field>
        <field name="model">auto_word_civil.windbase</field>
        <field name="arch" type="xml">
            <form string="风机基础数据表">
                <sheet>
                    <group name="group_top">
                         <group name="group_left">
                            <field name="FortificationIntensity" />
                            <field name="BearingCapacity" />
                            <field name="BasicType" />
                            <field name="UltimateLoad" />
                            <field name="FloorRadiusR" />
                            <field name="PileDiameter" />
                            <field name="M48PreStressedAnchor" />
                            <field name="C80SecondaryGrouting" />
                            <field name="Cushion" />
                         </group>
                         <group name="group_right">
                            <field name="R1" />
                            <field name="R2" />
                            <field name="H1" />
                            <field name="H2" />
                            <field name="H3" />
                            <field name="Number" />
                            <field name="Length" />
                            <field name="SinglePileLength" />
                            <field name="Area" />
                            <field name="Volume" />
                        </group>
                    </group>
                    <!--<group name="group_bottom">-->
                        <!--<field name="generator_Output" />-->
                    <!--</group>-->
                </sheet>
            </form>
        </field>
    </record>

    <record id="auto_word_civil_windbase_list" model="ir.ui.view">
        <field name="name">风机基础数据</field>
        <field name="model">auto_word_civil.windbase</field>
        <field name="arch" type="xml">
                <tree>
                    <field name="FortificationIntensity" />
                    <field name="BearingCapacity" />
                    <field name="BasicType" />
                    <field name="UltimateLoad" />
                    <field name="FloorRadiusR" />
                    <field name="PileDiameter" />
                    <field name="M48PreStressedAnchor" />
                    <field name="C80SecondaryGrouting" />
                    <field name="Cushion" />
                    <field name="R1" />
                    <field name="R2" />
                    <field name="H1" />
                    <field name="H2" />
                    <field name="H3" />
                    <field name="Number" />
                    <field name="Length" />
                    <field name="SinglePileLength" />
                    <field name="Area" />
                    <field name="Volume" />

                    <!--<group name="group_bottom">-->
                        <!--<field name="generator_Output" />-->
                    <!--</group>-->
                </tree>
        </field>
    </record>

    <record id="auto_word_civil_convertbase_form" model="ir.ui.view">
        <field name="name">箱变基础</field>
        <field name="model">auto_word_civil.convertbase</field>
        <field name="arch" type="xml">
            <form string="箱变基础表">
                <sheet>
                    <group name="group_top">
                        <group name="group_left">
                            <field name="TurbineCapacity" />
                            <field name="ConvertStation" />
                            <field name="Long" />
                            <field name="Width" />
                            <field name="High" />
                            <field name="WallThickness" />
                        </group>
                         <group name="group_right">
                            <field name="HighPressure" />
                            <field name="C35ConcreteTop" />
                            <field name="C15Cushion" />
                            <field name="MU10Brick" />
                            <field name="Reinforcement" />
                            <field name="Area" />
                        </group>
                    </group>
                    <!--<group name="group_bottom">-->
                        <!--<field name="generator_Output" />-->
                    <!--</group>-->
                </sheet>
            </form>
        </field>
    </record>

    <record id="auto_word_civil_boosterstation_form" model="ir.ui.view">
        <field name="name">升压站基础数据表</field>
        <field name="model">auto_word_civil.boosterstation</field>
        <field name="arch" type="xml">
            <form string="升压站基础数据表">
                <sheet>
                    <group name="group_top">
